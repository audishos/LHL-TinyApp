<%  if (username) {
%><div><%= username %></div>
<form action="/logout" method="POST">
  <input type="submit" value="Sign Out">
</form>
<%  } else { %>
<form action="/login" method="POST">
  <label for="username">user: </label>
  <input id="username" type="text" name="username">
  <input type="submit" value="Sign In">
</form>
<%  } %>
